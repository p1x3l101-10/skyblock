{
	"multipart": [
		{   "apply": { "model": "ic2:fence/iron_post" }},
		{   "when": { "north": "true" },
			"apply": { "model": "ic2:fence/iron_side", "uvlock": true }
		},
		{   "when": { "east": "true" },
			"apply": { "model": "ic2:fence/iron_side", "y": 90, "uvlock": true }
		},
		{   "when": { "south": "true" },
			"apply": { "model": "ic2:fence/iron_side", "y": 180, "uvlock": true }
		},
		{   "when": { "west": "true" },
			"apply": { "model": "ic2:fence/iron_side", "y": 270, "uvlock": true }
		}
	]
}

